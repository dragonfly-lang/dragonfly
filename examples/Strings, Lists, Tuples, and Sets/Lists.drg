
// Lists 
// Type signature: [<type>]
let names [string]
names = ["Jerry", "Barry", "Caesar"]

// List methods 
names.clear()
names.count()
names.find()
names.pop()
names.push()
names.extend()
names.insert()
names.reverse()
names.remove()

// Looping 
for name in names {}
for name, index in names {}

// Indexing 
names[0]

// Slicing [start:stop:step]
names[0:2:-1] = ["Barry", "Jerry"]